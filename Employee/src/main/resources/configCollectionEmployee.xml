       <list>
           <value>Java</value>
           <value>HTML</value>
           <value>CSS</value>
           <value>ReactJs</value>
       </list>

    </property>
    <property name="mobile">
        <set>
            <value>6307262902</value>
            <value>6392986560</value>
            <value>9839016359</value>
        </set>
    </property>
    <property name="address">
       <map>
           <entry key="45" value="Noida"/>
           <entry key="34" value="Delhi"/>
       </map>
    </property>


</bean>


</beans>